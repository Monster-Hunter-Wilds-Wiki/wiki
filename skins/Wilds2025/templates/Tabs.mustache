<div id="{{id}}" class="vector-menu vector-menu-tabs{{#class}} {{.}}{{/class}}" {{{html-tooltip}}} {{{html-user-language-attributes}}}>
  {{!>MenuContents}}
  <div class="vector-menu-content">
    {{{html-before-portal}}}
    <ul class="vector-menu-content-list">
      {{#array-list-items}}
        {{>MenuListItem}}
      {{/array-list-items}}
      {{#html-items}}{{{.}}}{{/html-items}}
    </ul>
    {{{html-after-portal}}}
  </div>
</div>