<div id="{{id}}" class="vector-menu vector-menu-tabs{{#class}} {{.}}{{/class}}" {{{html-tooltip}}} {{{html-user-language-attributes}}}>
  {{!>MenuContents}}
  <div class="vector-menu-content">
    {{{html-before-portal}}}
    <ul class="vector-menu-content-list">
      {{#array-list-items}}
        {{!>MenuListItem}}
        <li id="{{id}}" class="{{class}}">{{!
        }}{{#array-links}}{{!
          }}{{!>Link}}{{!}}
          <a data-mw="interface"{{#array-attributes}} {{key}}="{{value}}"{{/array-attributes}}>{{!
            }}{{#icon}}{{!>Icon}}{{/icon}}{{!}}
            <span class="vector-icon mw-ui-icon-{{.}} mw-ui-icon-wikimedia-{{.}}"></span>
            {{!}}<span>{{text}}</span>{{!
          }}</a>
        {{!}}{{/array-links}}{{!
        }}{{text}}{{!
        }}</li>
      {{/array-list-items}}
      {{#html-items}}{{{.}}}{{/html-items}}
    </ul>
    {{{html-after-portal}}}
  </div>
</div>